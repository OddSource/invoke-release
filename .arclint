{
  "linters": {
    "eb-python-relaxed-flake8": {
      "type": "flake8",
      "include": "(\\.py$)",
      "flags" : [
        "--max-line-length=120"
      ],
      "severity": {
        "E501": "advice"
      },
      "severity.rules": {
        "(^E)": "warning",
        "(^F)": "warning",
        "(^W)": "advice"
      }
    }
  }
}
